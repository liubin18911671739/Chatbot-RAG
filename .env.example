# .env.example

# Example environment variables for the RAG QA System

# Backend configuration
FLASK_ENV=development
FLASK_APP=run.py
SECRET_KEY=your_secret_key_here

# Database configuration
DATABASE_URL=mysql://username:password@mysql:3306/db_name

# Vector database configuration
FAISS_INDEX_PATH=/path/to/faiss/index

# Model service configuration
MODEL_SERVICE_URL=http://vllm:port

# Frontend configuration
FRONTEND_URL=http://localhost:8080

# Other configurations
UPLOAD_FOLDER=/path/to/uploaded_docs
EMBEDDINGS_FOLDER=/path/to/embeddings

# Set to true for debugging
DEBUG=True